    <game-info>
        <version>1.5.108</version>
        <region>EU</region>
        <ping-limit>150</ping-limit>
    </game-info>
    <settings username="default">   
        <zobrazeni>
            <display>
                <rozliseni>1920x1080</rozliseni>
                <monitor value="1" />
                <mode>Windowed-Fullscreen</mode>
                <gamma value="15" />
                <brightness value="50" />
                <colorblind-mode enabled="false" />
                <vsync enabled="false" />
                <screen-saver enabled="false" />
            </display>
            <texture-quality>Medium</texture-quality>
            <shadow-quality>Low</shadow-quality>
            <terrain-quality>Medium</terrain-quality>
            <water-quality>Low</water-quality>
            <foliage-quality>High</foliage-quality>
            <foliage-density>Low</foliage-density>
            <view-distance>High</view-distance>
            <anti-aliasing>MSAA</anti-aliasing>
            <motion-blur enabled="false" />
            <depth-of-field enabled="false" />
        </zobrazeni>
        <zvuk>
            <volume enabled="true">
                <main>50</main>
                <speech>20</speech>
                <music>10</music>
                <ambient>30</ambient>
                <effects>25</effects>
            </volume>
            <voicechat enabled="true">
                <voip-volume>33</voip-volume>
                <voip-mode>Push-to-talk</voip-mode>
            </voicechat>
            <speech lang="English" />
            <subtitles lang="Czech" enabled="true" />
        </zvuk>
        <hra>
            <ui-jazyk lang="English" />
            <aim-assistance enabled="false" />
            <transparent-ui>30</transparent-ui>
            <view-pet>My-Only</view-pet>     
            <hide-notifications>
                <combat-zone enabled="true" />
                <marketplace enabled="false" />
                <guild-war enabled="true" />
            </hide-notifications>
        </hra>
        <controls>
            <input>keyboard-mouse</input>
            <mouse-sensitivity>25</mouse-sensitivity>
            <mouse-smoothing enabled="false" />
            <mouse-inverted horizontal="false" vertical="false" />
        </controls>
    </settings>  
    
